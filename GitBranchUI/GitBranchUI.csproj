<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <PackageId>GitBranchUI</PackageId>
    <Version>0.1.0</Version>
    <Authors>Yauhen Pyl</Authors>
    <Company>Yauhen Pyl</Company>
    <PackageProjectUrl>https://github.com/ypyl/GitBranchUI/</PackageProjectUrl>
    <Description>This is a Razor component library with a git branch UI component.</Description>
    <License>MIT</License>
  </PropertyGroup>

  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="7.0.1" />
  </ItemGroup>

</Project>
